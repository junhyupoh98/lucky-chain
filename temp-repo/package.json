{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "predev": "node tools/convertSvgAttrs.cjs && node tools/convertStyleStrings.cjs || node tools/convertStyleStrings.js",
    "watch:convert": "chokidar \"src/**/*.{js,jsx,ts,tsx}\" -c \"node tools/convertSvgAttrs.cjs\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,json,md}\"",
    "convert-styles": "node tools/convertStyleStrings.js"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@babel/generator": "^7.28.3",
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "@babel/types": "^7.28.4",
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "chokidar-cli": "^3.0.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "glob": "^11.0.3",
    "globals": "^16.4.0",
    "prettier": "^2.8.8",
    "vite": "^7.1.7"
  }
}
